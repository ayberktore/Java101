import java.util.Scanner;

public class AlgoritmaveProgramlama {
    public static void main(String[] args){

        Scanner scan = new Scanner(System.in);
        int fizik,mat,kimya,turkce,tarih,muzik;
        String durum;
        String gectiniz="Sinifi Gecti";
        String kaldiniz="Sinifta Kaldi";

        System.out.println("Fizik notunuzu giriniz");
        fizik=scan.nextInt();
        System.out.println("Matematik notunuzu giriniz");
        mat=scan.nextInt();
        System.out.println("Kimya notunuzu giriniz");
        kimya= scan.nextInt();
        System.out.println("Turkce notunuzu giriniz");
        turkce= scan.nextInt();
        System.out.println("Tarih notunuzu giriniz");
        tarih=scan.nextInt();
        System.out.println("Muzik notunuzu giriniz");
        muzik=scan.nextInt();

        int toplam;
        toplam=(fizik+mat+kimya+turkce+tarih+muzik);
        double ortalama=toplam/6.0;
        boolean note = ortalama > 60;
        durum = note ? gectiniz : kaldiniz;
        System.out.println(durum);


    }
}
